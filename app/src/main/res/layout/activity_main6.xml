<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    layout_height="wrap_content"
    layout_width="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".MainActivity2">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="start"
        android:layout_marginTop="49dp" >

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#FFFFFF"
            android:textColor="#57A9FF"
            android:layout_marginLeft="300dp"
            android:onClick="onClickHander6"
            android:text="Пропустить" />


    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Создайте пароль"
        android:textSize="20dp"
        android:layout_marginTop="50dp"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="273dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Для защиты ваших персональных данных"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:srcCompat="@drawable/progress" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        android:gravity="center">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button2"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="1" />

            <Button
                android:id="@+id/button3"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="2" />

            <Button
                android:id="@+id/button7"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="3" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button8"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="4" />

            <Button
                android:id="@+id/button9"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="5" />

            <Button
                android:id="@+id/button10"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="6" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button11"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="7" />

            <Button
                android:id="@+id/button12"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="8" />

            <Button
                android:id="@+id/button13"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="9" />
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button14"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_marginRight="25dp"
                android:layout_marginLeft="150dp"
                app:cornerRadius="70dp"
                android:backgroundTint="#F5F5F9"
                android:textColor="@color/black"
                android:text="0" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="35dp"
                android:layout_height="24dp"
                android:layout_marginRight="50dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/del" />
        </LinearLayout>
    </LinearLayout>


</androidx.appcompat.widget.LinearLayoutCompat>